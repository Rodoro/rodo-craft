{
  "colorize.languages": [
    "javascript",
    "typescript",
  ],
  "audioCues.terminalCommandFailed": "on",
  "restoreTerminals.terminals": [{
      "splitTerminals": [{
        "name": "Client",
        "commands": ["cd frontend", "npm run dev"],
      }]
    },
    {
      "splitTerminals": [{
        "name": "Browser",
        "commands": ["Start-Process chrome.exe -ArgumentList \"http://localhost:3000\", \"http://localhost:3001/api\"", "exit"],
      }]
    },
    {
      "splitTerminals": [{
          "name": "Server",
          "commands": ["cd backend", "npm run start:dev"],
        },
        {
          "name": "Worker",
          "commands": ["cd backend"]
        }
      ]
    }
  ],
  "material-icon-theme.folders.customClones": [
    {
      "name": "dto",
      "base": "class",
      "color": "purple-400",
      "folderNames": ["dto"]
    },
    {
      "name": "exception",
      "base": "private",
      "color": "red-600",
      "folderNames": ["exceptions", "exception"]
    },
    {
      "name": "roles",
      "base": "context",
      "color": "purple-400",
      "folderNames": ["role", "roles"]
    },
    {
      "name": "users",
      "base": "admin",
      "color": "green-400",
      "folderNames": ["users"]
    },
    {
      "name": "vscode",
      "base": "vscode",
      "color": "blue-400",
      "folderNames": [".VSCodeCounter"]
    },
  ],
  "material-icon-theme.files.customClones": [
    {
      "name": "Dto",
      "base": "dll",
      "color": "blue-400",
      "fileExtensions": ["dto.ts", "dto.js"]
    },
    {
      "name": "Nest-mo",
      "base": "nest",
      "color": "red-400",
      "fileExtensions": ["module.ts", "module.js"]
    },
    {
      "name": "Nest-s",
      "base": "nest",
      "color": "yellow-400",
      "fileExtensions": ["service.ts", "service.js"]
    },
    {
      "name": "Nest-co",
      "base": "nest",
      "color": "blue-400",
      "fileExtensions": ["controller.ts", "controller.js"]
    },
    {
      "name": "Schema",
      "base": "database",
      "color": "brown-400",
      "fileExtensions": ["schema.ts", "schema.js"]
    },
    {
      "name": "Nest-guard",
      "base": "nest",
      "color": "green-400",
      "fileExtensions": ["guard.ts", "guard.js"]
    },
    {
      "name": "Nest-pipes",
      "base": "nest",
      "color": "green-800",
      "fileExtensions": ["pipe.ts", "pipe.js"]
    },
    {
      "name": "Nest-exception",
      "base": "nest",
      "color": "red-600",
      "fileExtensions": ["exception.ts", "exception.js"]
    },
  ]
}